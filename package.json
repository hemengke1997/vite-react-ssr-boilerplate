{
  "name": "vite-react-ssr-boilerplate",
  "version": "0.0.0",
  "author": "hemengke1997 <https://github.com/hemengke1997>",
  "description": "vite ssr boilerplate",
  "keywords": [
    "vite",
    "ssr",
    "template",
    "boilerplate",
    "vite-plugin-ssr"
  ],
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "simple-git-hooks",
    "start": "tsx scripts/dev.ts",
    "build": "cross-env NODE_ENV=production vite build --mode production && vite build --ssr --mode production",
    "build:test": "cross-env NODE_ENV=test vite build --mode test && vite build --ssr --mode test",
    "start:test": "cross-env API_PREFIX=/THEAPI NODE_ENV=test tsx server/index.ts",
    "start:prod": "cross-env NODE_ENV=production tsx server/index.ts",
    "ssr": "cross-env API_PREFIX=/THEAPI NODE_ENV=development tsx server/index.ts",
    "preview": "vite preview",
    "report": "cross-env REPORT=true vite build",
    "bump:deps": "pnpm update --i --L",
    "lint": "eslint .",
    "//": "以下部署相关",
    "bundleServer": "tsx scripts/bundleServer",
    "release": "tsx scripts/release",
    "pm2:test": "npx pnpm i && cross-env NODE_ENV=test && tsx scripts/pm2",
    "pm2:prod": "npx pnpm i && cross-env NODE_ENV=production && tsx scripts/pm2"
  },
  "engines": {
    "node": ">=16.14.2"
  },
  "dependencies": {
    "antd": "^4.22.6",
    "axios": "^0.27.2",
    "clsx": "^1.2.1",
    "dayjs": "^1.11.5",
    "lodash-es": "^4.17.21",
    "normalize-path": "^3.0.0",
    "query-string": "^7.1.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@minko-fe/commitlint-config": "^1.1.1",
    "@minko-fe/eslint-config": "^1.1.1",
    "@minko-fe/postcss-pxtorem": "^0.0.5",
    "@minko-fe/prettier-config": "^1.1.1",
    "@minko-fe/tsconfig": "^1.1.1",
    "@types/compression": "^1.7.2",
    "@types/cross-spawn": "^6.0.2",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^9.0.1",
    "@types/lodash-es": "^4.17.6",
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.7.6",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@types/react-helmet": "^6.1.5",
    "@types/shelljs": "^0.8.11",
    "@vitejs/plugin-legacy": "2.0.1",
    "@vitejs/plugin-react": "2.0.1",
    "autoprefixer": "^10.4.7",
    "compression": "^1.7.4",
    "consola": "^2.15.3",
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.3",
    "dotenv": "^16.0.1",
    "esbuild": "^0.15.5",
    "eslint": "^8.22.0",
    "eslint-define-config": "^1.6.0",
    "execa": "^6.1.0",
    "express": "^4.18.1",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.1.0",
    "http-proxy-middleware": "^2.0.6",
    "inquirer": "^9.0.2",
    "less": "^4.1.3",
    "minimist": "^1.2.6",
    "open": "^8.4.0",
    "picocolors": "^1.0.0",
    "pm2": "^5.2.0",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prompts": "^2.4.2",
    "rollup": "^2.78.0",
    "rollup-plugin-visualizer": "^5.7.1",
    "shelljs": "^0.8.5",
    "simple-git-hooks": "^2.8.0",
    "sirv": "^2.0.2",
    "tailwindcss": "^3.1.6",
    "terser": "5.14.2",
    "tsx": "^3.8.2",
    "typescript": "^4.7.4",
    "vite": "^3.0.9",
    "vite-plugin-ssr": "^0.4.20",
    "vite-plugin-style-import": "^2.0.0"
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm exec tsx scripts/verifyCommit.ts $1"
  }
}
