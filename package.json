{
  "name": "vite-react-ssr-boilerplate",
  "version": "0.0.0",
  "author": "hemengke1997 <https://github.com/hemengke1997>",
  "description": "ssr boilerplate based on vite/vite-plugin-ssr/react",
  "keywords": [
    "vite",
    "ssr",
    "template",
    "boilerplate",
    "vite-plugin-ssr",
    "react ssr",
    "vite ssr"
  ],
  "type": "module",
  "scripts": {
    "vite": "vite",
    "dev": "tsx scripts/dev.ts",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "simple-git-hooks",
    "ssr": "cross-env NODE_ENV=development tsx server/index.ts",
    "start:prod": "cross-env NODE_ENV=production tsx server/index.ts",
    "start:test": "cross-env NODE_ENV=test tsx server/index.ts",
    "build": "cross-env NODE_ENV=production vite build --mode production && vite build --ssr --mode production",
    "build:test": "cross-env NODE_ENV=test vite build --mode test && vite build --ssr --mode test",
    "test:build": "cross-env NODE_ENV=test vite build --mode test",
    "bump:deps": "pnpm update --i --L",
    "init": "simple-git-hooks",
    "lint": "eslint . --fix",
    "preview": "vite preview",
    "release": "tsx scripts/release",
    "report": "cross-env REPORT=true vite build",
    "bundleServer": "tsx scripts/bundleServer",
    "//": "以下部署相关",
    "pm2:dev": "npx --yes --ignore-scripts pnpm i && cross-env NODE_ENV=development tsx server/pm2 --tsconfig tsconfig.pm2.json",
    "pm2:test": "npx --yes --ignore-scripts pnpm i -P && cross-env NODE_ENV=test tsx server/pm2 --tsconfig tsconfig.pm2.json",
    "pm2:prod": "npx --yes --ignore-scripts pnpm i -P && cross-env NODE_ENV=production tsx server/pm2 --tsconfig tsconfig.pm2.json"
  },
  "dependencies": {
    "@minko-fe/style-object-to-string": "^0.0.1",
    "@minko-fe/tsconfig": "^1.2.3",
    "ahooks": "^3.7.2",
    "antd": "^4.24.0",
    "axios": "0.27.2",
    "clsx": "^1.2.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.6",
    "express": "^4.18.2",
    "headroom.js": "^0.12.0",
    "http-proxy-middleware": "^2.0.6",
    "lodash-es": "^4.17.21",
    "normalize-path": "^3.0.0",
    "normalize-url": "^8.0.0",
    "normalize.css": "^8.0.1",
    "picocolors": "^1.0.0",
    "pm2": "5.2.2",
    "qrcode.react": "3.1.0",
    "query-string": "^7.1.1",
    "react": "18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "18.2.0",
    "react-helmet": "^6.1.0",
    "react-transition-group": "^4.4.5",
    "react-vant": "^3.0.3",
    "sirv": "^2.0.2",
    "tailwindcss": "^3.2.2",
    "tsx": "^3.11.0",
    "vite": "npm:@minko-fe/vite@^3.2.1",
    "vite-plugin-ssr": "^0.4.46",
    "zustand": "^4.1.4"
  },
  "devDependencies": {
    "@minko-fe/commitlint-config": "^1.2.3",
    "@minko-fe/eslint-config": "^1.2.3",
    "@minko-fe/postcss-pxtorem": "^0.0.11",
    "@minko-fe/prettier-config": "^1.2.3",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/compression": "^1.7.2",
    "@types/cross-spawn": "^6.0.2",
    "@types/express": "^4.17.14",
    "@types/fs-extra": "^9.0.13",
    "@types/headroom.js": "^0.12.2",
    "@types/inquirer": "^9.0.3",
    "@types/less": "^3.0.3",
    "@types/lodash-es": "^4.17.6",
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.11.9",
    "@types/normalize-path": "^3.0.0",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "@types/react-helmet": "^6.1.5",
    "@types/react-transition-group": "^4.4.5",
    "@types/shelljs": "^0.8.11",
    "@vitejs/plugin-legacy": "2.3.1",
    "@vitejs/plugin-react": "2.2.0",
    "autoprefixer": "^10.4.12",
    "browserslist": "^4.21.4",
    "consola": "^2.15.3",
    "esbuild": "^0.15.13",
    "eslint": "^8.27.0",
    "eslint-define-config": "^1.11.0",
    "execa": "^6.1.0",
    "fast-glob": "^3.2.12",
    "fs-extra": "^10.1.0",
    "get-tsconfig": "^4.2.0",
    "inquirer": "^9.1.4",
    "less": "^4.1.3",
    "lint-staged": "^13.0.3",
    "magic-string": "^0.26.7",
    "minimist": "^1.2.7",
    "ora": "^6.1.2",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "prompts": "^2.4.2",
    "rollup": "2.79.1",
    "rollup-plugin-visualizer": "^5.8.3",
    "shelljs": "^0.8.5",
    "simple-git-hooks": "^2.8.1",
    "terser": "5.15.1",
    "typescript": "^4.8.4",
    "vite-plugin-public-typescript": "^0.0.9",
    "vite-plugin-style-import": "^2.0.0",
    "vite-plugin-time-reporter": "^2.0.0"
  },
  "engines": {
    "node": ">=16.14.2"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:@minko-fe/vite"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged --concurrent false",
    "commit-msg": "pnpm exec tsx scripts/verifyCommit.ts $1"
  },
  "lint-staged": {
    "*": [
      "prettier --write --cache --ignore-unknown"
    ],
    "!(dist/**)/*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix"
    ]
  }
}
